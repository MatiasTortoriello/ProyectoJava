program{
    unaLista := [1,2,3,4,5]
    return(posiciónDe_enLaQueAparece_(unaLista, 3))
}

function posiciónDe_enLaQueAparece_(unaLista, elemento){
    /*

    */
    lista := unaLista
    contador := 0
    while(primero(lista) /= elemento ){
        lista := sinElPrimero(lista)
        contador := contador + 1
    }
    return(contador)
}